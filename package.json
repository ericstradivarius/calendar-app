        "lodash": "^4.17.4",
        "vue": "^2.1.10"
    },
    "dependencies": {
        "angular": "^1.6.6",
        "angular-datetime-input": "^5.2.1",
        "angular-material-time-picker": "^1.0.7",
        "angular-moment-picker": "^0.10.1",
        "angular-ui-bootstrap": "^2.5.6",
        "moment": "^2.19.2",
        "ng-file-upload": "^12.2.13",
        "pug": "^2.0.0-rc.4",
        "swiper": "^4.0.5"
